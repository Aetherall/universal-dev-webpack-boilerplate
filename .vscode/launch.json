	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Start dev server",
			"program": "${workspaceRoot}/omni/build/bin/run.dev.js",
			"protocol": "inspector",
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"outFiles": ["${workspaceRoot}/omni/build/dist/**/*.js"],
			"skipFiles": ["${workspaceRoot}/node_modules/**/*.js", "${workspaceRoot}/omni/build/dist/**/*.js", "<node_internals>/**/*.js"]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Start dev server n_m",
			"program": "${workspaceRoot}/omni/build/bin/run.dev.js",
			"protocol": "inspector",
			"sourceMaps": true,
			"cwd": "${workspaceRoot}",
			"outFiles": ["${workspaceRoot}/omni/build/dist/**/*.js"],
			"skipFiles": ["${workspaceRoot}/omni/build/dist/**/*.js", "<node_internals>/**/*.js"]
		}
	]
}
